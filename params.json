이름\" : \"김서영\", \"참여도\" : 3},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0}\r\n                        ]\r\n                    },\r\n                    {\r\n                        \"게임명\" : \" \",\r\n                        \"참여인물\" : [ \r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0},\r\n                            {\"이름\" : \"\", \"참여도\" : 0}\r\n                         ]\r\n                    }\r\n                ]\r\n\r\n        var width = 960,\r\n            height = 550,\r\n            radius = 200;\r\n        \r\n        var arc = d3.svg.arc()\r\n            .innerRadius(radius-40)\r\n            .outerRadius(radius);\r\n        \r\n        var pie = d3.layout.pie()\r\n            .padAngle(.02);\r\n        \r\n        var color = d3.scale.category20c();\r\n        \r\n        var svg = d3.select(\"#box\").append(\"svg\")\r\n            .attr(\"width\", width)\r\n            .attr(\"height\", height)\r\n            .append(\"g\")\r\n            .attr(\"transform\", \"translate(\" + width / 3.3   + \",\" + height / 2 + \")\");\r\n        \r\n        svg.selectAll(\"path\"                                                                                  